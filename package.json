        "copy-webpack-plugin": "^9.0.1",
        "cross-env": "^7.0.3",
        "css-loader": "^5.2.4",
        "css-minimizer-webpack-plugin": "^2.0.0",
        "eslint": "^7.24.0",
        "eslint-config-airbnb-base": "^14.2.1",
        "eslint-plugin-import": "^2.22.1",
        "file-loader": "^6.2.0",
        "html-loader": "^2.1.2",
        "html-webpack-plugin": "^5.3.2",
        "less": "^4.1.1",
        "less-loader": "^9.0.0",
        "mini-css-extract-plugin": "^1.6.2",
        "node-sass": "^6.0.1",
        "postcss": "^8.3.6",
